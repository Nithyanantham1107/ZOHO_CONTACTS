<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>ZOHO_CONTACTS</display-name>
	<welcome-file-list>

		<welcome-file>Login.jsp</welcome-file>

	</welcome-file-list>


	<filter>
		<filter-name>Sessionfilter</filter-name>
		<filter-class>com.zohocontacts.filter.SessionFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>Sessionfilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>


	<listener>

		<listener-class>com.zohocontacts.listener.ServerListener</listener-class>
	</listener>



	<servlet>
		<servlet-name>user</servlet-name>
		<servlet-class>com.zohocontacts.servlets.UserServlet</servlet-class>

	</servlet>
	<servlet-mapping>
		<servlet-name>user</servlet-name>
		<url-pattern>/user</url-pattern>
	</servlet-mapping>




	<servlet>
		<servlet-name>group</servlet-name>
		<servlet-class>com.zohocontacts.servlets.GroupServlet</servlet-class>

	</servlet>
	<servlet-mapping>
		<servlet-name>group</servlet-name>
		<url-pattern>/group</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>contact</servlet-name>
		<servlet-class>com.zohocontacts.servlets.ContactServlet</servlet-class>

	</servlet>
	<servlet-mapping>
		<servlet-name>contact</servlet-name>
		<url-pattern>/contact</url-pattern>
	</servlet-mapping>



	<servlet>
		<servlet-name>logout</servlet-name>
		<servlet-class>com.zohocontacts.servlets.LogoutServlet</servlet-class>

	</servlet>
	<servlet-mapping>
		<servlet-name>logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.zohocontacts.servlets.LoginSignupServlet</servlet-class>

	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>



	<servlet>
		<servlet-name>servercache</servlet-name>
		<servlet-class>com.zohocontacts.servlets.ServerCacheDeleteServlet</servlet-class>

	</servlet>
	<servlet-mapping>
		<servlet-name>servercache</servlet-name>
		<url-pattern>/servercache</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>sessioncache</servlet-name>
		<servlet-class>com.zohocontacts.servlets.SessionCacheHandleServlet</servlet-class>

	</servlet>
	<servlet-mapping>
		<servlet-name>sessioncache</servlet-name>
		<url-pattern>/sessioncache</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>syncstate</servlet-name>
		<servlet-class>com.zohocontacts.servlets.OauthSyncStateServlet</servlet-class>

	</servlet>
	<servlet-mapping>
		<servlet-name>syncstate</servlet-name>
		<url-pattern>/syncstate</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>signup</servlet-name>
		<servlet-class>com.zohocontacts.servlets.SignupServlet</servlet-class>

	</servlet>
	<servlet-mapping>
		<servlet-name>signup</servlet-name>
		<url-pattern>/signup</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>oauthdirector</servlet-name>
		<servlet-class>com.zohocontacts.servlets.GoogleOauthdirectorServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>oauthdirector</servlet-name>
		<url-pattern>/oauthdirector</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>oauthredirect</servlet-name>
		<servlet-class>com.zohocontacts.servlets.GoogleOauthRedirectServlet</servlet-class>

	</servlet>
	<servlet-mapping>
		<servlet-name>oauthredirect</servlet-name>
		<url-pattern>/oauthredirect</url-pattern>
	</servlet-mapping>




</web-app>